    102          /**
    103          * @brief  USB_OTG_BSP_EnableInterrupt
    104          *         Enabele USB Global interrupt
    105          * @param  None
    106          * @retval None
    107          */
    108          
    109          
    110          
    111          void USB_OTG_BSP_EnableInterrupt(USB_OTG_CORE_HANDLE *pdev)
    112          {
    113            NVIC_InitTypeDef NVIC_InitStructure; 
    114            
    115            NVIC_PriorityGroupConfig(NVIC_PriorityGroup_1);
    116          
    117            NVIC_InitStructure.NVIC_IRQChannel = OTG_FS_IRQn;  
    118          
    119            NVIC_InitStructure.NVIC_IRQChannelPreemptionPriority = 1;
    120            NVIC_InitStructure.NVIC_IRQChannelSubPriority = 3;
    121            NVIC_InitStructure.NVIC_IRQChannelCmd = ENABLE;
    122            NVIC_Init(&NVIC_InitStructure);  
    123          }
    124          /**
    125          * @brief  USB_OTG_BSP_uDelay
    126          *         This function provides delay time in micro sec
    127          * @param  usec : Value of delay required in micro sec
    128          * @retval None
    129          */
    130          void USB_OTG_BSP_uDelay (const uint32_t usec)
    131          {
    132            uint32_t count = 0;
    133            const uint32_t utime = (120 * usec / 7);
    134            do
    135            {
    136              if ( ++count > utime )
    137              {
    138                return ;
    139              }
    140            }
    141            while (1);
    142          }
    143          
    144          
    145          /**
    146          * @brief  USB_OTG_BSP_mDelay
    147          *          This function provides delay time in milli sec
    148          * @param  msec : Value of delay required in milli sec
    149          * @retval None
    150          */
    151          void USB_OTG_BSP_mDelay (const uint32_t msec)
    152          {
    153            USB_OTG_BSP_uDelay(msec * 1000);   
    154          }
